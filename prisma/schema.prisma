generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model doctors {
  name           String
  contactNumber  String @id @unique
  email          String
  specialization String
  yoe            Int
  schedule       String
}
